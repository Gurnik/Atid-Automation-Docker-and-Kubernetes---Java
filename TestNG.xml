<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="3">
    <test name="Selenium Tests with chrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="tests.SanityTestHubNodes" />
        </classes>
    </test> <!-- Test -->

    <test name="Selenium Tests with ff">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="tests.SanityTestParamsGridAutoSetup" />
        </classes>
    </test> <!-- Test -->

    <test name="Selenium Tests with edge">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="tests.SanityTestParamsGridAutoSetup" />
        </classes>
    </test> <!-- Test -->
</suite> <!-- Suite -->